<% if logged_in? %>
  <div class="text-center">
    <h1>タスク一覧</h1>
    <%= render "tasks", tasks: @tasks %>
    <%= link_to "新規タスクの登録", new_task_path,class: "btn btn-primary" %>
  </div>
<% else%>
  <div class="text-center">
    <%= link_to "Sign up", signup_path, class: "btn btn-lg btn-primary" %>
  </div>
<% end %>





